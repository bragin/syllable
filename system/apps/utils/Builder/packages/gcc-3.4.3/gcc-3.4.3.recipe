authors
	Kurt Skauen
	Kaj de Vos
	Arno Klenke
	Kristian Van Der Vliet
maintainers			Kaj de Vos
status				testing

licenses			GPL LGPL GFDL
home				gcc.gnu.org
;packages
;	gcc-core-3.4.3
;	gcc-g++-3.4.3
;	gcc-objc-3.4.3
;	gcc-java-3.4.3
;	gcc-g77-3.4.3
;	gcc-ada-3.4.3
;	gcc-testsuite-3.4.3
locations			ftp://gcc.gnu.org/pub/gcc/releases/gcc-3.4.3/

needs
;	Due to new atomics definitions:
	Syllable >= 0.5.5
;	For -O3 with -fPIC:
	binutils >= 2.13.1
;	Syllable switched to the new Glibc ABI in combination with this version of Gcc:
	glibc >= 2.3.3
build-needs
;	autoconf 2.57
;	Without make bootstrap:
	gcc >= 2.95
;	For Ada:
;	gnat >= 3.13 | gcc >= 3.1
	gawk
test-needs			dejagnu >= 1.4.1

patch-config		. boehm-gc
;patch
;	cd gcc
;	autoconf


; When compiling with GCC 2.95.x installed in the system:
;pre-configure		rm -f /system/libs/libstdc++.so
; Dirty alternative for replacing libstdc++.so, when compiling with GCC 2.95.x:
;configure-env		CXXFLAGS '-march=i586 -O2 -pipe -lstdc++'
configure-options
	--enable-languages=c,c++
;		,treelang
;		Won't do --enable-shared:
;		,objc
;		,java,ada
	--with-arch=i586
	--enable-sjlj-exceptions
	--enable-shared
	--enable-threads
	--with-system-zlib
	--disable-libstdcxx-pch
;	--with-local-prefix=/usr/local

make-env
	BOOT_CFLAGS		'-march=i586 -O -pipe'
;	For make bootstrap when compiling with GCC 2.95.x:
;	BOOT_CFLAGS		"-march=i586 -O -pipe -L$PWD/gcc"
;	DLL_PATH		"$DLL_PATH:$PWD/gcc"
make-vars
	CFLAGS			'-march=i586 -O -pipe'
	LIBCFLAGS		'-march=i586 -O2 -pipe'
	LIBCXXFLAGS		'-march=i586 -O2 -pipe'
;					GCC seems to set this automatically:
;					' -fno-implicit-templates'
make-targets
;	When compiling with GCC 3.x:
	bootstrap
;	For printed documentation:
;	dvi

;test-env
;	TCL_LIBRARY		'/usr/tcl'
;	DEJAGNULIBS		'/usr/dejagnu'
test-options		-k

;install-prefix-var	DESTDIR
;system-files
;	/system/libs
;		lib/libgcc_s.so*
;		lib/libstdc++.so*
documentation
	README FAQ faq.html BUGS bugs.html COPYING COPYING.LIB NEWS ChangeLog MAINTAINERS
	gcc/ABOUT-GCC-NLS
links
	gcc						bin/cc
	libgcc.a				lib/gcc/i586-pc-syllable/3.4.3/libgcc_static.a
system-links
	lib/libgcc_s.so.2		/system/libs/libgcc_s.so.2
	libgcc_s.so.2			/system/libs/libgcc_s.so
;	lib/libstdc++.so.6.0.3	/system/libs/libstdc++.so.6.0.3
;	libstdc++.so.6.0.3		/system/libs/libstdc++.so.6
;	libstdc++.so.6.0.3		/system/libs/libstdc++.so

